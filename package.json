{
  "name": "angularis",
  "version": "1.0.26",
  "description": "Angular Library Suite",
  "author": "Lovelidge, Shawn",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "clean:web": "rm -rf dist/web",
    "build:core": "ng build @angularis/core",
    "build:model": "ng build @angularis/model",
    "build:pipe": "ng build @angularis/pipe",
    "build:service": "ng build @angularis/service",
    "build:directive": "ng build @angularis/directive",
    "build:component": "ng build @angularis/component",
    "build:web": "ng build web",
    "build:storybook": "ng run @angularis/storybook:build-storybook",
    "build:prebuild": "sync-json -v --property version --source package.json --targets  ./projects/core/package.json ./projects/model/package.json ./projects/pipe/package.json ./projects/service/package.json ./projects/directive/package.json ./projects/component/package.json",
    "build:schematics": "ng build @angularis/schematics && npm run build:schematics:post-build",
    "build:schematics:post-build": "cp -R projects/schematics/src/* dist/schematics/src/ && rm -rf dist/schematics/src/**/*.spec.ts && rm -rf dist/schematics/src/**/index.ts",
    "build": "npm run clean && npm run build:prebuild && npm run build:core && npm run build:model && npm run build:pipe && npm run build:service && npm run build:directive && npm run build:component",
    "start": "npm run clean:web && npm run build:web && ng serve web",
    "storybook": "ng run @angularis/storybook:storybook",
    "test:core": "jest --passWithNoTests --config projects/core/jest.config.ts",
    "test:core:coverage": "jest --passWithNoTests --config projects/core/jest.config.ts --coverage && open projects/core/coverage/index.html",
    "test:model": "jest --passWithNoTests --config projects/model/jest.config.ts",
    "test:model:coverage": "jest --passWithNoTests --config projects/model/jest.config.ts --coverage && open projects/model/coverage/index.html",
    "test:pipe": "jest --passWithNoTests --config projects/pipe/jest.config.ts",
    "test:pipe:coverage": "jest --passWithNoTests --config projects/pipe/jest.config.ts --coverage && open projects/pipe/coverage/index.html",
    "test:service": "jest --passWithNoTests --config projects/service/jest.config.ts",
    "test:service:coverage": "jest --passWithNoTests --config projects/service/jest.config.ts --coverage && open projects/service/coverage/index.html",
    "test:directive": "jest --passWithNoTests --config projects/directive/jest.config.ts",
    "test:directive:coverage": "jest --passWithNoTests --config projects/directive/jest.config.ts --coverage && open projects/directive/coverage/index.html",
    "test:component": "jest --passWithNoTests --config projects/component/jest.config.ts",
    "test:component:coverage": "jest --passWithNoTests --config projects/component/jest.config.ts --coverage && open projects/component/coverage/index.html",
    "test": "npm run test:core && npm run test:model && npm run test:pipe && npm run test:service && npm run test:directive && npm run test:component",
    "test:schemantic:jest": "npx schematics ./projects/schematics/src/collection.json:jest --name=test-file"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.6",
    "@angular/cdk": "^20.0.5",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@fortawesome/angular-fontawesome": "^2.0.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.5",
    "@angular/build": "^20.0.5",
    "@angular/cli": "^20.0.3",
    "@angular/compiler-cli": "^20.0.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.30.1",
    "@jest/globals": "^30.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/parser": "^8.35.1",
    "angular-eslint": "^20.1.1",
    "babel-jest": "^30.0.4",
    "eslint": "^9.30.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.6.0",
    "ng-packagr": "^20.0.0",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "sass-embedded": "^1.89.2",
    "sass-loader": "^16.0.5",
    "sass-migrator": "^2.3.3",
    "style-loader": "^4.0.0",
    "sync-json": "^1.0.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.35.1"
  }
}